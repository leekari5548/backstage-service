<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leekari.dao.TypeClassDao">

    <resultMap type="com.leekari.entity.TypeClass" id="BaseResultMap">
        <id property="id" column="id" jdbcType="VARCHAR"/>
        <result property="typeKey" column="type_key" jdbcType="VARCHAR"/>
        <result property="typeName" column="type_name" jdbcType="VARCHAR"/>
        <result property="createUser" column="create_user" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="deleted" column="is_deleted" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="baseColumn">
        id,type_key,type_name,create_user,create_time,is_deleted
    </sql>

    <select id="getAllTypeClass" resultMap="BaseResultMap" >
        select <include refid="baseColumn"></include>
        from type_class
        where is_deleted = 0
    </select>

</mapper>